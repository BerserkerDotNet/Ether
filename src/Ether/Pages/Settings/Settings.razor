@page "/settings"

<div class="mat-layout-grid">
    <div class="mat-layout-grid-inner">
        <div class="mat-layout-grid-cell mat-layout-grid-cell-span-12">
            @Identities
        </div>
        <div class="mat-layout-grid-cell mat-layout-grid-cell-span-12">
            @Organizations
        </div>
    </div>
</div>

@code{

	private RenderFragment Identities = IdentitiesConnected.Get();

	private RenderFragment Organizations = OrganizationsConnected.Get();

}